"1	LINE	0
	---
	- name: first playbook
	  hosts: all
	  tasks:
	  - name: install httpd
	    yum: name=httpd
	  - name: run httpd service
	    service: name=httpd state=started
	  - name: get service facts
	    service_facts:
	  - name: check httpd status
	    debug:
	      var: ansible_facts.services["httpd.service"]
	  - name: ensure file exists
	    copy:
	      content: "Welcome to my web server"
	      dest: /var/www/html/index.html
	      force: no
	      group: sys
	      owner: root
	      mode: 0555
	  - name: run firewalld service
	    service: name=firewalld state=started
	  - name: get service facts
	    service_facts:
	  - name: check firewalld status
	    debug:
	      var: ansible_facts.services["firewalld.service"]
	  - name: open https
	    firewalld:
	      service: https
	      permanent: true
	      state: enabled
	  - name: open port 443
	    firewalld:
	      port: 443/tcp
	      permanent: yes
	      state: enabled
	  - name: open port 80
	    firewalld:
	      port: 80/tcp
	      permanent: yes
	      state: enabled
	  - name:  NetworkManager
	    service: name=NetworkManager enabled=false
	  - name: find net.ifnames
	    shell: cat /etc/default/grub
	    register: grub_cat
	  - replace:
	      path: /etc/default/grub
